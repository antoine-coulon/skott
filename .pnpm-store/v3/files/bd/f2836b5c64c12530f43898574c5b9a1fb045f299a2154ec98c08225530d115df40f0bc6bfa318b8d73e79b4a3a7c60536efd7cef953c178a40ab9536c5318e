import * as Data from "@effect/data/Data";
/** @internal */
export const isNone = fa => fa._tag === "None";
/** @internal */
export const isSome = fa => fa._tag === "Some";
/** @internal */
export const none = /*#__PURE__*/Data.struct({
  _tag: "None"
});
/** @internal */
export const some = a => Data.struct({
  _tag: "Some",
  value: a
});
