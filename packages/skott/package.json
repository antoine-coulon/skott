{
  "name": "skott",
  "version": "0.20.2",
  "description": "Automatically construct and visualize Graphs generated from your JavaScript and TypeScript project",
  "author": "Antoine Coulon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antoine-coulon/skott.git"
  },
  "bugs": {
    "url": "https://github.com/antoine-coulon/skott/issues"
  },
  "main": "dist/index.js",
  "bin": {
    "skott": "./dist/bin/cli.js"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./filesystem/*": "./dist/src/filesystem/*.js",
    "./modules/*": "./dist/src/modules/*.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc --project tsconfig.build.json",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run test && pnpm run build",
    "prestart": "pnpm run build",
    "start": "node dist/bin/cli.js",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@effect/data": "^0.4.1",
    "@effect/io": "^0.8.0",
    "@typescript-eslint/typescript-estree": "^5.48.0",
    "compression": "^1.7.4",
    "depcheck": "^1.4.3",
    "digraph-js": "^2.0.2",
    "estree-walker": "^3.0.1",
    "fp-ts": "2.5.0",
    "fs-tree-structure": "workspace:*",
    "ignore-walk": "^6.0.3",
    "io-ts": "^2.2.20",
    "json5": "^2.2.2",
    "kleur": "^4.1.5",
    "ligie": "^0.0.6",
    "lodash.difference": "^4.5.0",
    "memfs": "^3.4.6",
    "meriyah": "^4.2.1",
    "openurl": "^1.1.1",
    "ora": "^6.1.2",
    "polka": "^0.5.2",
    "sade": "^1.8.1",
    "sirv": "^2.0.2",
    "skott-webapp": "workspace:^",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@nodesecure/eslint-config": "^1.3.0",
    "@skott/config": "workspace:*",
    "@types/compression": "^1.7.2",
    "@types/ignore-walk": "^4.0.0",
    "@types/lodash.difference": "^4.5.7",
    "@types/node": "^16.11.12",
    "@types/polka": "^0.5.4",
    "@types/sade": "^1.7.4",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "vitest": "^0.28.2"
  },
  "files": [
    "dist",
    "./README.md"
  ],
  "keywords": [
    "javascript",
    "graph",
    "nodejs",
    "visualization",
    "module",
    "dependencies"
  ]
}
