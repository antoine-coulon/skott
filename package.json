{
  "name": "cyclops",
  "version": "0.0.1",
  "description": "Automatically construct and visualize Graphs generated from your Node.js project",
  "main": "dist/index.js",
  "type": "module",
  "author": "Antoine Coulon",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run build",
    "prepublishOnly": "npm run test && npm run build",
